package ch06;

import java.awt.Dialog;
import java.awt.Frame;

import javax.swing.JFrame;

class This5{
	int a;
	String str;
	
	public This5() {
		
	}
	public This5(int a) {
		this.a = a;
	}
	public This5(String str) {
		this.str = str;
	}
	public This5(int a, String str) {
		this.str = str;
		this.a = a;
	}
}

class This6{
	int a;
	String str;
	
	This6(int a){
		// super() 생략 안되어있음 -> 동일한 Object 클래스가 생성되기 때문
		this(a,"익명");
	}
	
	public This6(int a, String str) {
		super();
		this.a = a;
		this.str = str;
	}
	
}


public class ConstructorEx5{

	public static void main(String[] args) {

	}

}
