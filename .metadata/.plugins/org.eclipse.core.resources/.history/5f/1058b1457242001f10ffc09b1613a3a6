package ch01;

public class Ex3 {
	public static void main(String[] args) {
		// 기본형 8개 + 참조형 무한대(ex. String)
		
		// 정수형 4
		byte b = 10;
		short s = 10;
		int i = 20;
		long l = 10;
		
		// 실수형 2
		float f = 3.14f; // 리터럴 값에 반드시 뒤에 f / F 표기
		double d = 3.14; // 기본
		
		// 문자형 1
		
		
		// 불린형 1
	}
}
