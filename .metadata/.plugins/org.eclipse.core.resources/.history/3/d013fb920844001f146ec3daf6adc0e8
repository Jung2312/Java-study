package ch06;

class Method2<T>{
	void prn(T a) {
		System.out.println(a);
	}
	void prn(double a) {
		System.out.println(a);
	}
	void prn(float a) {
		System.out.println(a);
	}
	void prn(String a) {
		System.out.println(a);
	}
	
	
}



public class MethodEx2 {

	public static void main(String[] args) {
		Method2 m1 = new Method2();
		m1.prn(4.5);
	}

}
