package ch06;

class Method2<T>{
	
	void prn(double a) {
		System.out.println(a);
	}
	void prn(float a) {
		System.out.println(a);
	}
	void prn(String a) {
		System.out.println(a);
	}
	
	
	void prn(T a) {
		System.out.println(a);
	}
	
	void prn(T a, T b) {
		System.out.println(a +" " + b);
	}
}

public class MethodEx2 {

	public static void main(String[] args) {
		Method2 m1 = new Method2();
		m1.prn(2);
		m1.prn(2,4);
	}

}
