package ch04;

public class Ex3 {

	public static void main(String[] args) {
		String month = "february";
		
		switch (month) {
		case "june": {
			System.out.println("6월");
			break;
		}
		
		case "july": {
			System.out.println("6월");
			break;
		}
		
		case "march": {
			break;
		}
		
		default:
			throw new IllegalArgumentException("Unexpected value: " + month);
		}
	}

}
